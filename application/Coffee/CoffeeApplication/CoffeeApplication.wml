<div class="coffee-application">
   <Router.Route mask="page/:pageId" attr:class="coffee-application-content">
      <Coffee.PageLoader default="Info" temp="{{content.pageId}}">
         <ws:templateOptions
            settingsInfo="{{settingsInfo}}"
            currentInfo="{{currentInfo}}"/>
      </Coffee.PageLoader>
   </Router.Route>
</div>
<div class="info-page tile-view">
   <div class="info-list info-tile">
      <ws:for data="name, infoElement in currentInfo">
         <div class="info-list-item">
            <div class="info-list-item-name">{{name}}:</div>
            <div class="info-list-item-state">{{infoElement.value}}</div>
         </div>
      </ws:for>
   </div>
   <Router.Reference state="page/:pageId" pageId="Settings" class="settings-button">
      <div></div>
   </Router.Reference>
</div>

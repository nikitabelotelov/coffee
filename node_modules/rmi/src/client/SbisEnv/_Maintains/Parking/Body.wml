<div class="SbisEnv-ParkingBody {{ size? 'SbisEnv-Parking__size_' + size: ''}}">
    <ws:if data="{{ _options.image }}">
        <img src="{{ _options.image }}" class="SbisEnv-ParkingBody__image" />
    </ws:if>
    <ws:if data="{{ _options.message }}">
        <div class="SbisEnv-ParkingBody__message">
            <ws:partial template="{{ _options.message }}" />
        </div>
    </ws:if>
    <ws:if data="{{ _options.details }}">
        <div class="SbisEnv-ParkingBody__details">
            <ws:partial template="{{ _options.details }}" />
        </div>
    </ws:if>
    <ws:if data="{{ _options.action && size == 'large' }}">
        <div class="SbisEnv-ParkingBody__action">
            <ws:partial template="{{ _options.action }}" />
        </div>
    </ws:if>
</div>

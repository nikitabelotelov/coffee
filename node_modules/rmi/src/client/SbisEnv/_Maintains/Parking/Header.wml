<div class="SbisEnv-ParkingPage__Header">
    <div class="SbisEnv-ParkingPage__Header-center ws-flexbox ws-align-items-baseline">
        <a href="https://sbis.ru/" class="SbisEnv-ParkingPage__Header-leftBlock ws-flexbox ws-align-items-baseline">
            <img class="SbisEnv-ParkingPage__Header-logo" src="{{ _options.logo }}"/>
            <span class="SbisEnv-ParkingPage__Header-title">{{ rk(_options.title) }}</span>
        </a>
        <div class="SbisEnv-ParkingPage__Header-rightBlock ws-flex-grow-1 ws-flexbox ws-justify-content-end ws-align-items-baseline">
            <div if="{{ _options.lang }}" class="SbisEnv-ParkingPage__Header-langWrapper">
                <EngineLanguage.Selector locale="_options.locale"/>
            </div>
        </div>
    </div>
</div>
